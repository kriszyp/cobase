{
  "name": "cobase",
  "author": "Kris Zyp",
  "version": "0.10.8",
  "description": "Compositional data stores of reactive JavaScript transform, index, reduce, and join functions, built on NodeJS, Alkali, and LMDB",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "http://github.com/kriszyp/cobase"
  },
  "scripts": {
    "test": "mocha tests/unit -u tdd",
    "build": "tsc || rollup -c"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./index.js": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./dist/util/WeakValueMap": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./dist/util/when": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "dependencies": {
    "msgpackr": "^1.4.4",
    "lmdb": "^2.0.0-beta3"
  },
  "peerDependencies": {
    "alkali": "^1.1.2"
  },
  "optionalDependencies": {
    "weak-lru-cache": "^1.1.3"
  },
  "devDependencies": {
    "@types/node": "latest",
    "chai": "^4",
    "fs-extra": "^9.0.0",
    "mocha": "^5",
    "typescript": "^4.4.3",
    "rollup": "^1.20.3"
  }
}
